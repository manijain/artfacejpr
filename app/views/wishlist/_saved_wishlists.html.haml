    - if @new_offers[index].blank?
      %h3.pull-left="&nbsp; #{wishlist.wishlist_name}".html_safe
    - else
      %h3.pull-left="&nbsp; #{wishlist.wishlist_name}.".html_safe
      %h4.pull-left{:style=>"padding-top:12px;padding-left:10px;"}="#{@new_offers[index]} new paintings have been added since #{wishlist.updated_at.to_date}"
