%select#select_input_artist_.pull-left{:style=>"margin-bottom: 0;margin-right: 5px;width: 100px",:name=>"search_params[artist]search_type"}
  %option{:value=>"contains"}="contains"
  %option{:value=>"is exactly"}="is exactly"
  %option{:value=>"is not"}="is not"
%input#text_input_artist_{:type=>"text", :style=>"float: left;margin-bottom: 0;width: 200px;" ,:name=>"search_params[artist]value"}
%img#error_text_input_artist_{"data-original-title"=>"Only Characters allowed", :src=>"/images/exclamation_mark.png", :style=>"display:none;position:absolute;right:170px;"}
